{
  "name": "bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Maxim Rudin",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "dev": "npx nodemon",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.2",
    "@fastify/view": "^11.1.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.0.0",
    "handlebars": "^4.7.8",
    "inversify": "^7.5.1",
    "jwa": "^2.0.1",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jwa": "^2.0.3",
    "@types/node": "^20.17.32",
    "@types/pg": "^8.15.2",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.44.0",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
